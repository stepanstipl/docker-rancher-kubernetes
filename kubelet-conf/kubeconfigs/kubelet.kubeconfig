apiVersion: v1
kind: Config
clusters:
  - cluster:
      certificate-authority: /srv/kubernetes/ca.pem
    name: kubernetes
contexts:
  - context:
      cluster: kubernetes
      user: kubelet
    name: kubelet
current-context: kubelet
users:
  - name: kubelet
    user:
      client-certificate: /srv/kubernetes/client.pem
      client-key: /srv/kubernetes/client-key.pem
